<module>
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.i18n.I18N" />

	<source>
		<include name="util/DateRange.java" />
		<include name="util/IDescriptorProvider.java" />
		<include name="util/INameValueProvider.java" />
		<include name="model/schema/PropertyType.java" />
		<include name="model/EntityType.java" />
		<include name="model/impl/AccountStatus.java" />
		<include name="model/impl/AddressType.java" />
		<include name="model/impl/CreditCardType.java" />
		<include name="criteria/CriteriaType.java" />
		<include name="criteria/SelectNamedQuery.java" />
		<include name="criteria/IQueryParam.java" />
		<include name="listhandler/ListHandlerType.java" />
		<include name="listhandler/IPage.java" />
		<include name="listhandler/Page.java" />
		<include name="listhandler/PageUtil.java" />
		<include name="listhandler/SortDir.java" />
		<include name="listhandler/SortColumn.java" />
		<include name="listhandler/Sorting.java" />
	</source>

	<replace-with class="com.tll.client.ui.field.impl.FieldLabelImplIE">
		<when-type-is class="com.tll.client.ui.field.impl.FieldLabelImpl" />
		<any>
			<when-property-is name="user.agent" value="ie6" />
		</any>
	</replace-with>

	<replace-with class="com.tll.client.ui.impl.GlassPanelImplOpera">
		<when-type-is class="com.tll.client.ui.impl.GlassPanelImpl" />
		<when-property-is name="user.agent" value="opera" />
	</replace-with>

	<replace-with class="com.tll.client.ui.impl.GlassPanelImplSafari">
		<when-type-is class="com.tll.client.ui.impl.GlassPanelImpl" />
		<when-property-is name="user.agent" value="safari" />
	</replace-with>

	<replace-with class="com.tll.client.ui.impl.GlassPanelImplIE6">
		<when-type-is class="com.tll.client.ui.impl.GlassPanelImpl" />
		<when-property-is name="user.agent" value="ie6" />
	</replace-with>

	<replace-with class="com.tll.client.ui.impl.GlassPanelImplMozilla">
		<when-type-is class="com.tll.client.ui.impl.GlassPanelImpl" />
		<any>
			<when-property-is name="user.agent" value="gecko1_8" />
			<when-property-is name="user.agent" value="gecko" />
		</any>
	</replace-with>

</module>