<module>
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.i18n.I18N" />

	<replace-with class="com.tll.client.ui.field.impl.FieldLabelImplIE">
		<when-type-is class="com.tll.client.ui.field.impl.FieldLabelImpl" />
		<any>
			<when-property-is name="user.agent" value="ie6" />
		</any>
	</replace-with>

	<replace-with class="com.tll.client.ui.impl.GlassPanelImplOpera">
		<when-type-is class="com.tll.client.ui.impl.GlassPanelImpl" />
		<when-property-is name="user.agent" value="opera" />
	</replace-with>

	<replace-with class="com.tll.client.ui.impl.GlassPanelImplSafari">
		<when-type-is class="com.tll.client.ui.impl.GlassPanelImpl" />
		<when-property-is name="user.agent" value="safari" />
	</replace-with>

	<replace-with class="com.tll.client.ui.impl.GlassPanelImplIE6">
		<when-type-is class="com.tll.client.ui.impl.GlassPanelImpl" />
		<when-property-is name="user.agent" value="ie6" />
	</replace-with>

	<replace-with class="com.tll.client.ui.impl.GlassPanelImplMozilla">
		<when-type-is class="com.tll.client.ui.impl.GlassPanelImpl" />
		<any>
			<when-property-is name="user.agent" value="gecko1_8" />
			<when-property-is name="user.agent" value="gecko" />
		</any>
	</replace-with>

</module>