<?xml version="1.0" encoding="UTF-8" ?>
<jdoquery xmlns="http://java.sun.com/xml/ns/jdo/jdoquery" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/jdo/jdoquery
