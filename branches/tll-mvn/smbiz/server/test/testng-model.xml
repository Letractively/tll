<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="smbiz-model-tests" verbose="5">
  
  <test name="model.schema">
    <groups>
      <run>
        <include name="model.schema" />
      </run>
    </groups>
    <packages>
      <package name="com.tll.model.schema" />
    </packages>
  </test>

  <test name="model.key">
    <groups>
      <run>
        <include name="model.key" />
      </run>
    </groups>
    <packages>
      <package name="com.tll.model.key" />
    </packages>
  </test>

  <test name="model.validator">
    <groups>
      <run>
        <include name="model.validate" />
      </run>
    </groups>
    <packages>
      <package name="com.tll.model.validate" />
    </packages>
  </test>

</suite>