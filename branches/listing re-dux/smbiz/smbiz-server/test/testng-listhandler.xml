<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="smbiz-listhandler-tests" verbose="5">
  
  <test name="paging">
    <classes>
      <class name="com.tll.service.entity.PagingSearchListHandlerTest" />
    </classes>
  </test>
  
  <test name="listhandler.namedqueries.orm">
    <parameter name="jpaMode" value="SPRING" />
    <parameter name="daoMode" value="ORM" />
    <classes>
      <class name="com.tll.listhandler.NamedQueryDataRetrievalTest" />
    </classes>
  </test>

  <test name="listhandler.namedqueries.mock">
    <parameter name="jpaMode" value="NONE" />
    <parameter name="daoMode" value="MOCK" />
    <classes>
      <class name="com.tll.listhandler.NamedQueryDataRetrievalTest" />
    </classes>
  </test>

</suite>