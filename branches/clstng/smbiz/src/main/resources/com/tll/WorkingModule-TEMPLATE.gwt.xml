<!--
 This is a template file. It should be copied and re-named so that the '-TEMPLATE' token is absent.
-->
<module rename-to="com.tll.SmbizAdminApp">
  <inherits name="com.tll.SmbizAdminApp" />
  
  <!-- set user.agent to ie6 for host mode compatibility -->
  <set-property name="user.agent" value="ie6" />
  <set-property name="locale" value="default" />
  
  <!-- Add gwt-log support, default level `DEBUG` -->
  <inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" />

  <stylesheet src="css/admin.css" />
</module>
